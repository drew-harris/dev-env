[[manager.prepend_keymap]]
on   = "l"
run  = "plugin smart-enter"
desc = "Enter the child directory, or open the file"

[[manager.prepend_keymap]]
on   = "f"
run  = "plugin jump-to-char"
desc = "Jump to char"

[[manager.prepend_keymap]]
on   = "<C-f>"
run  = "filter"
desc = "filter"

[[manager.prepend_keymap]]
on   = "C"
run  = ['yank', 'paste']
desc = "Duplicate and rename"

[[manager.prepend_keymap]]
on   = "r"
run  = ['rename --empty=stem --cursor=start']
desc = "Better rename"
